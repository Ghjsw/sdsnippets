{
  "name": "sd-snippets",
  "displayName": "sdsnippets",
  "description": "sd frontend snippets",
  "publisher": "BOYA",
  "version": "0.0.8",
  "engines": {
    "vscode": "^1.0.0"
  },
  "devDependencies": {
    "vscode": "0.10.x"
  },
  "activationEvents": [
    "onLanguage:vuejs"
  ],
  "main": "./out/extension",
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/snippets.code-snippets"
      },
      {
        "language": "html",
        "path": "./snippets/snippets.code-snippets"
      },
      {
        "language": "vue",
        "path": "./snippets/snippets.code-snippets"
      },
      {
        "language": "vue-html",
        "path": "./snippets/snippets.code-snippets"
      }
    ]
  }
}
